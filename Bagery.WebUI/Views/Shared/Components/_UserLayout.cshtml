            darkMode: "class",
            theme: {
                extend: {
                    colors: {
                        "primary": "#f42559",
                        "primary-soft": "#ffeaef",
                        "secondary": "#9c495e",
                        "background-light": "#FFFBF2", /* Soft cream as requested */
                        "background-dark": "#2d1a1e",
                        "card-light": "#FFFFFF",
